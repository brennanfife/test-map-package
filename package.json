{
  "name": "testmappackage",
  "version": "1.0.0",
  "description": "Maps package for the WeMeta marketplace",
  "keywords": [
    "ethereum",
    "web3",
    "metaverse",
    "maps",
    "decentraland",
    "the-sandbox",
    "somnium-space",
    "cryptovoxels"
  ],
  "engines": {
    "node": "14.18.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brennanfife/test-map-package.git"
  },
  "main": "dist/index.js",
  "files": [
    "/dist",
    "package.json"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "rm package-lock.json && rm -rf node_modules && rm -rf dist",
    "lint": "tsdx lint src",
    "prepare": "tsdx build && husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@monsonjeremy/react-leaflet": "^3.2.2",
    "axios": "^0.24.0",
    "framer-motion": "^4.1.17",
    "leaflet": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-leaflet": "^3.2.2",
    "react-scripts": "4.0.3",
    "react-tile-map": "^0.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@types/axios": "^0.14.0",
    "@types/leaflet": "^1.7.7",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "8.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "tsdx": "^0.14.1",
    "typescript": "^4.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
